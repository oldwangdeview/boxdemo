<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/select_frame_lay"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#ffffff">


    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/topbar_lay"
        android:layout_marginTop="@dimen/dp_30"
        android:orientation="vertical">


        <ListView
            android:id="@+id/selsectFrameLV"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:divider="#E5E5E5"
            android:dividerHeight="1px"
            android:footerDividersEnabled="false"
            android:gravity="center_vertical"
            android:headerDividersEnabled="false"
            android:layout_marginBottom="@dimen/dp_10"
            android:scrollbars="none" />

    </LinearLayout>


    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="#f4f4f4"/>
    <LinearLayout
        android:id="@+id/topbar_lay"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_45"
        android:layout_alignParentBottom="true"
        android:orientation="horizontal"
        >
        <TextView
            android:id="@+id/fram_reset_but"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:textSize="@dimen/sp_16"
            android:textColor="@color/c_262626"
            android:text="重置"/>

        <TextView
            android:id="@+id/fram_ok_but"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:textSize="@dimen/sp_16"
            android:textColor="@color/c_ffffff"
            android:background="@color/c_d52e21"
            android:text="确认"/>
    </LinearLayout>

</RelativeLayout>
